\section{Modules}

\begin{frame}{Modules}

\begin{itemize}
  \item Login and TPO module (TEAM 2)
  \item Student module (TEAM 3)
  \item Faculty module (TEAM 4)
  \item Resume Builder (TEAM 1)
\end{itemize}



\end{frame}
\section{Resume-Builder(TEAM 1)}

\begin{frame}{Resume-Builder(TEAM 1)}
\begin{itemize}

   \item Accessing data from student details
   \item Inbuilt easy-pdf or build-pdf function to generate resume
   \item Option to download resume (pdf file)
 
\end{itemize}
\end{frame}
\section{Student module(TEAM 3)}

\begin{frame}{Student module(TEAM 3)}
\begin{itemize}
   \item Editing student profile
   \item Eligibility criteria to companies
   \item Company Details
   \item Rating student skills by faculty
\end{itemize}
\end{frame}
\section{login and TPO module(TEAM 2)}

\begin{frame}{login and TPO module(TEAM 2)}
\begin{itemize}
   \item login for students, faculty, admin
   \item  accessing TPO  
   \item placement Details
 
\end{itemize}
\end{frame}

\section{Faculty module(TEAM 4)}

\begin{frame}{Faculty module(TEAM 4)}
\begin{itemize}
  \item Access to all student profiles
  \item Viewing all student details. 
  \item Remarks
\end{itemize}
\end{frame}

